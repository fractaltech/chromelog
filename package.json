{
  "name": "chromelog",
  "version": "1.0.2",
  "description": "Log stuff from anywhere into chrome",
  "repository": "https://github.com/fractaltech/chromelog",
  "main": "client.js",
  "scripts": {
    "start": "./server.js",
    "build": "npm run clean && babel src/ --out-dir lib/",
    "watch": "npm run clean && babel -w src/ --out-dir lib/",
    "clean": "rm -rf lib/",
    "dist": "npm run build && cp -rf lib/* . && chmod 755 server.js",
    "dist-clean": "npm run clean && rm ./*.js"
  },
  "bin": {
    "chromelog": "./server.js"
  },
  "author": "kapil verma <kapv89@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "socket.io": "^1.4.5",
    "superagent": "^1.8.3"
  }
}
